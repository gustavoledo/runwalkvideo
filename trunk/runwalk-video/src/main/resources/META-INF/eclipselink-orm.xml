<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings version="2.1" xmlns="http://www.eclipse.org/eclipselink/xsds/persistence/orm" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://www.eclipse.org/eclipselink/xsds/persistence/orm http://www.eclipse.org/eclipselink/xsds/eclipselink_orm_2_1.xsd">
	<persistence-unit-metadata>
		<xml-mapping-metadata-complete />
		<persistence-unit-defaults>
			<schema>testdb</schema>
			<access>FIELD</access>
		</persistence-unit-defaults>
	</persistence-unit-metadata>
	<package>com.runwalk.video.entities</package>
	<entity class="Client">
		<attributes>
			<id name="id">
				<column name="customer"/>
			</id>
			<basic name="name">
			</basic>
			<basic name="taxNumber">
				<column name="vat_no"/>
			</basic>
			<basic name="phoneNumber">
				<column name="telephone"/>
			</basic>
			<basic name="firstname">
				<column name="contact_name"/>
			</basic>
			<basic name="inMailingList">
				<column name="mail_yn"/>
			</basic>
			<basic name="emailAdress">
				<column name="email"/>
			</basic>
			<basic name="birthDate">
				<column name="birthday"/>
				<temporal>DATE</temporal>
			</basic>
			<basic name="gender">
				<column name="treatment"/>
				<enumerated>STRING</enumerated>
			</basic>
			<one-to-many name="analyses" mapped-by="client">
				<cascade>
					<cascade-all />
				</cascade>
				<join-fetch>OUTER</join-fetch>
			</one-to-many>
			<embedded name="address">
				<attribute-override name="address">
					<column name="street_no"/>
				</attribute-override>
				<attribute-override name="postalcode">
					<column name="zipcode"/>
				</attribute-override>
			</embedded>
			<transient name="lastAnalysisDate"/>
			<transient name="version"/>
			<transient name="organization"/>
		</attributes>
	</entity>
	<entity class="Analysis">
		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
			<basic name="comments">
				<lob />
			</basic>
			<basic name="creationDate">
				<column name="date"/>
				<temporal>DATE</temporal>
			</basic>
			<many-to-one name="client">
				<join-column name="client_id" nullable="false"/>
			</many-to-one>
			<one-to-many name="recordings" mapped-by="analysis">
				<cascade>
					<cascade-all />
				</cascade>
				<join-fetch>OUTER</join-fetch>
			</one-to-many>
			<one-to-one name="article">
				<join-column name="article_id"/>
			</one-to-one>
		</attributes>
	</entity>
	<entity class="City">
		<attributes>
			<id name="code">
				<column name="zipcode"/>
				<generated-value />
			</id>
			<basic name="name">
				<column name="city"/>
			</basic>
			<transient name="id"/>
		</attributes>
	</entity>
	<entity class="Recording">
		<attributes>
			<id name="id">
				<generated-value strategy="IDENTITY"/>
			</id>
			<basic name="duration">
			</basic>
			<basic name="statusCode">
			</basic>
			<basic name="lastModified">
				<column name="lastmodified"/>
			</basic>
			<basic name="videoFileName">
			</basic>
			<many-to-one name="analysis">
				<join-column name="analysis_id" nullable="false"/>
			</many-to-one>
			<one-to-many name="keyframes" mapped-by="recording">
				<cascade>
					<cascade-all />
				</cascade>
			</one-to-many>
			<transient name="recordingStatus"/>
		</attributes>
	</entity>
	<entity class="Keyframe">
		<attributes>
			<id name="id">
				<generated-value />
			</id>
			<basic name="position">
			</basic>
			<many-to-one name="recording">
				<join-column name="recording_id" nullable="false"/>
			</many-to-one>
		</attributes>
	</entity>
</entity-mappings>
